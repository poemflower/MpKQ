/* components/divider/divider.wxss */
.mpkq-divider_box{
  padding: 5px 10% 4px;
  text-align: center;
}
.mpkq-divider_label{
  text-align: center;
  position: relative;
  width: 100%;
  display: block;
  font-size:22rpx;
  line-height:48rpx;
  font-weight:500;
  color:#000;
}
.mpkq-divider_label::after, .mpkq-divider_label::before {
    content: "";
    position: absolute;
    top: 50%;
    width: 15%;
    height: 1px;
    background-color: #ddd; 
}
.mpkq-divider_label::before{
  left: 0;
}
.mpkq-divider_label::after{
  right: 0;
}